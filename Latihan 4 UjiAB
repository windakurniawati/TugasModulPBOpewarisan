package Pewarisan;

public class UjiAB {
    public static void main(String[] args) {
        B ily = new B();
        ily.setA(100);
        ily.setB(150);
        //akan memanggil method yang terdapat pada kelas B
        ily.tampilkanNilai();
    }
    
}

