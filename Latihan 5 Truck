package Pewarisan;


public class Truck 
{
//the Truck class has three frields
public int cadence;
public int gear;
public int speed;
    //Kelas Truck mempunyai 1 konstruktor
public Truck(int startCadence, int startSpeed, int startGear)
{
    gear = startGear;
    cadence = startCadence;
    speed = startSpeed;
    
}
//Kelas Truck mempunyai 4 method
public void setCadence(int newValue)
{
    cadence = newValue;
}
public void setGear(int newValue)
{
    gear = newValue;
}
public void applybrake(int decrement)
{
    speed = decrement;
}
public void speedUp(int increment)
{
    speed = increment;
}
}
